from rest_framework import serializers
from .models import KnowledgeGraphBase, KnowledgeGraph


class KnowledgeGraphBaseSerializer(serializers.ModelSerializer):
    class Meta:
        model = KnowledgeGraphBase
        fields = '__all__'


class KnowledgeGraphSerializer(serializers.ModelSerializer):
    class Meta:
        model = KnowledgeGraph
        fields = '__all__'
